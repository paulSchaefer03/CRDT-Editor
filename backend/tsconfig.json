    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "rootDir": "./"
    },
    "include": ["*.ts"]
}